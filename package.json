{
  "name": "hydra-idp-management",
  "version": "1.0.0",
  "description": "Digital Rogue Wave",
  "author": "Nizar Chaouachi",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "migration:create": "ts-node node_modules/.bin/typeorm migration:create",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "cache:clear": "ts-node ./node_modules/.bin/typeorm cache:clear"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/terminus": "^7.0.0",
    "@nestjs/typeorm": "^7.0.0",
    "@types/argon2": "^0.15.0",
    "argon2": "^0.27.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "helmet": "^3.22.0",
    "pg": "^8.0.0",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.0.7",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.17.4",
    "@types/express-serve-static-core": "^4.16.0",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.11.1",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "6.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
